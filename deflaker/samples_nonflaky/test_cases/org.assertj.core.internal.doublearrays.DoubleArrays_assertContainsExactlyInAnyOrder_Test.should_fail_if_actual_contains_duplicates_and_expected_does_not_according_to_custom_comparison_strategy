@Test
public void should_fail_if_actual_contains_duplicates_and_expected_does_not_according_to_custom_comparison_strategy() {
    actual = arrayOf(1d, 2d, 3d);
    double[] expected = { 1d, 2d };
    assertThatExceptionOfType(AssertionError.class).isThrownBy(() -> arraysWithCustomComparisonStrategy.assertContainsExactlyInAnyOrder(someInfo(), actual, expected)).withMessage(format(shouldContainExactlyInAnyOrder(actual, expected, emptyList(), newArrayList(3d), absValueComparisonStrategy).create()));
}
