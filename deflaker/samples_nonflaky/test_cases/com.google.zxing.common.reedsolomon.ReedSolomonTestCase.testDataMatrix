@Test
public void testDataMatrix() {
    // real life test cases
    testEncodeDecode(GenericGF.DATA_MATRIX_FIELD_256, new int[] { 142, 164, 186 }, new int[] { 114, 25, 5, 88, 102 });
    testEncodeDecode(GenericGF.DATA_MATRIX_FIELD_256, new int[] { 0x69, 0x75, 0x75, 0x71, 0x3B, 0x30, 0x30, 0x64, 0x70, 0x65, 0x66, 0x2F, 0x68, 0x70, 0x70, 0x68, 0x6D, 0x66, 0x2F, 0x64, 0x70, 0x6E, 0x30, 0x71, 0x30, 0x7B, 0x79, 0x6A, 0x6F, 0x68, 0x30, 0x81, 0xF0, 0x88, 0x1F, 0xB5 }, new int[] { 0x1C, 0x64, 0xEE, 0xEB, 0xD0, 0x1D, 0x00, 0x03, 0xF0, 0x1C, 0xF1, 0xD0, 0x6D, 0x00, 0x98, 0xDA, 0x80, 0x88, 0xBE, 0xFF, 0xB7, 0xFA, 0xA9, 0x95 });
    // synthetic test cases
    testEncodeDecodeRandom(GenericGF.DATA_MATRIX_FIELD_256, 10, 240);
    testEncodeDecodeRandom(GenericGF.DATA_MATRIX_FIELD_256, 128, 127);
    testEncodeDecodeRandom(GenericGF.DATA_MATRIX_FIELD_256, 220, 35);
}
