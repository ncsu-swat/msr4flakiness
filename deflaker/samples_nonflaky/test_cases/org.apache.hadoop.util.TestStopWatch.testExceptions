@Test
public void testExceptions() throws Exception {
    StopWatch sw = new StopWatch();
    try {
        sw.stop();
    } catch (Exception e) {
        Assert.assertTrue("IllegalStateException is expected", e instanceof IllegalStateException);
    }
    sw.reset();
    sw.start();
    try {
        sw.start();
    } catch (Exception e) {
        Assert.assertTrue("IllegalStateException is expected", e instanceof IllegalStateException);
    }
}
