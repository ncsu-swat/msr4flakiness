@Test
public void should_fail_if_actual_contains_duplicates_and_expected_does_not() {
    AssertionInfo info = someInfo();
    actual = arrayOf(1, 2, 3);
    int[] expected = { 1, 2 };
    try {
        arrays.assertContainsExactlyInAnyOrder(info, actual, expected);
    } catch (AssertionError e) {
        verify(failures).failure(info, shouldContainExactlyInAnyOrder(actual, expected, emptyList(), newArrayList(3), StandardComparisonStrategy.instance()));
        return;
    }
    failBecauseExpectedAssertionErrorWasNotThrown();
}
