@Test
public void should_return_description_of_FieldByFieldComparator_without_field_comparators() {
    assertThat(fieldByFieldComparator).hasToString(format("field/property by field/property comparator on all fields/properties%n" + "Comparators used:%n" + "- for elements fields (by type): {Double -> DoubleComparator[precision=1.0E-15], Float -> FloatComparator[precision=1.0E-6]}"));
}
