@Test
public void should_allow_assertions_on_three_extracted_values_from_given_iterable_by_using_a_function() {
    assertThat(fellowshipOfTheRing).extracting(TolkienCharacter::getName, TolkienCharacter::getAge, TolkienCharacter::getRace).containsOnly(tuple("Frodo", 33, TolkienCharacter.Race.HOBBIT), tuple("Sam", 38, HOBBIT), tuple("Gandalf", 2020, MAIA), tuple("Legolas", 1000, ELF), tuple("Pippin", 28, HOBBIT), tuple("Gimli", 139, DWARF), tuple("Aragorn", 87, MAN), tuple("Boromir", 37, MAN));
}
