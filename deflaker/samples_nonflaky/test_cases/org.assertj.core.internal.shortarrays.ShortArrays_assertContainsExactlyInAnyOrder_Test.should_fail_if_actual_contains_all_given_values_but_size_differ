@Test
public void should_fail_if_actual_contains_all_given_values_but_size_differ() {
    AssertionInfo info = someInfo();
    short[] expected = { 6, 8 };
    try {
        arrays.assertContainsExactlyInAnyOrder(info, actual, expected);
    } catch (AssertionError e) {
        verify(failures).failure(info, shouldContainExactlyInAnyOrder(actual, expected, emptyList(), newArrayList((short) 10), StandardComparisonStrategy.instance()));
        return;
    }
    failBecauseExpectedAssertionErrorWasNotThrown();
}
